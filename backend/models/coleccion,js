import mongoose from "mongoose";

const nftSchema = mongoose.Schema({
    name:{
        type: String,
        trim: true,
        required: true,
    },
    creatorId:{
        type: mongoose.Schema.Types.ObjectId,
        ref: "Usuario",
    }
},{
    timestamps: true,
})

const NftCreated = mongoose.model("NftCreated", nftSchema)
export default NftCreated;